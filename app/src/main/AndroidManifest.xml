<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joiner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/jobr_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/jobr_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".LoginPageActivity"></activity>
        <activity android:name=".SelectorEmpleadosActivity"></activity>
        <activity android:name=".RRHHActivity"></activity>
        <activity android:name=".FirebaseTestActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>